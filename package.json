{
	"name": "react-electron-starter",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"electron": "^7.1.1",
		"express": "^4.17.1",
		"lodash": "^4.17.15",
		"react": "^16.11.0",
		"react-dom": "^16.11.0",
		"react-redux": "^7.1.3",
		"react-scripts": "3.2.0",
		"react-spring": "^8.0.27",
		"redux": "^4.0.4",
		"redux-devtools-extension": "^2.13.8",
		"systeminformation": "^4.14.17",
		"tailwindcss": "^1.1.3"
	},
	"scripts": {
		"start": "nf start -p 3000",
		"build": "react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject",
		"electron": "electron .",
		"electron-start": "npm run tailwind:css && nodemon src/start-react",
		"react-start": "cross-env BROWSER=none react-scripts start",
		"pack": "build --dir",
		"dist": "npm run build && build",
		"postinstall": "install-app-deps",
		"dev": "npm run react-start && npm run electron-start",
		"api:dev": "cross-env NODE_ENV=dev nodemon ./src/api",
		"tailwind:css": "tailwind build src/css/tailwind.src.css -c  tailwind.js -o src/css/tailwind.css"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@testing-library/react": "^9.3.2",
		"cross-env": "^6.0.3",
		"electron-builder": "^22.1.0",
		"nodemon": "^1.19.4"
	},
	"homepage": "./",
	"main": "src/start.js",
	"build": {
		"appId": "com.electron.electron-with-create-react-app",
		"win": {
			"iconUrl": "https://cdn2.iconfinder.com/data/icons/designer-skills/128/react-256.png"
		},
		"directories": {
			"buildResources": "public"
		}
	},
	"proxy": "http://localhost:5000"
}
